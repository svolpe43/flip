{
  "manifest_version": 2,

  "name": "Flipper",
  "description": "Super cool awesome workspace manager for Chrome.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "res/icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "commands": {}
}
